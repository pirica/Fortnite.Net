<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <LangVersion>9</LangVersion>
        <Title>Fortnite.Net</Title>
        <PackageProjectUrl>https://github.com/thoo0224/Fortnite.Net</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/thoo0224/Fortnite.Net</PackageLicenseUrl>
        <PackageIconUrl>https://pbs.twimg.com/profile_images/1349105931603292162/2GoYgBnC_400x400.jpg</PackageIconUrl>
        <RepositoryUrl>https://github.com/thoo0224/Fortnite.Net/blob/master/LICENSE</RepositoryUrl>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>Fortnite.Net.Thoo</PackageId>
        <PackageVersion>1.0.2</PackageVersion>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="RestSharp" Version="106.11.8-alpha.0.11" />
      <PackageReference Include="RestSharp.Serializers.NewtonsoftJson" Version="106.11.8-alpha.0.11" />
    </ItemGroup>

</Project>
